#include <Servo.h>

Servo myservo; 
int pos = 100;
const int Servo = 1;
const int buttonPin = 2;
//int servoVal;
int LEDa = 10;
int LEDb = 11;


void setup() {
myservo.attach(9);
pinMode(buttonPin, INPUT);
pinMode(LEDa, OUTPUT);
pinMode(LEDb, OUTPUT);

}

void loop() {
  if (digitalRead(buttonPin) == HIGH) {
myservo.write(100);
digitalWrite(LEDa, LOW);
digitalWrite(LEDb, LOW);
  } else {
myservo.write(0);
digitalWrite(LEDa, HIGH);
digitalWrite(LEDb, HIGH);
  }
}
